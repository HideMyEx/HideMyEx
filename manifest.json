{
  "manifest_version": 2,

  "name": "HideMyEx",
  "description": "A self-control extension to save you from yourself.",
  "version": "0.0.01",

  "browser_action": {
    "default_icon": "/assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}